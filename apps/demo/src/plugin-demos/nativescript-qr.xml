<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="nativescript-qr" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <GridLayout class="p-20" rows="auto,auto">
        <Image src="{{ qr }} "/>
        <Button row="1" text="Generate Qr" tap="{{ generateQr }}" class="btn btn-primary"/>
    </GridLayout>
</Page>